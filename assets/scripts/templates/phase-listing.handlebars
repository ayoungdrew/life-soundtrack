{{#each phases as |phase|}}

 <!-- Developers, why are we using data-id and not just id? -->
  <ul>
    <li>
      <h1 data-id="{{phase.id}}">
        <small>
          Name: {{phase.name}}
          ID: {{phase.id}}
          <!-- {{phase.songs}} -->
        </small>
      </h1>
    </li>
    <li>
      {{#if phase.start_date}}
        Start Date: {{phase.start_date}}
      {{else}}
        No start date provided
      {{/if}}
    </li>
    <li>
      <button type="button" class="phase-delete-button" data-id={{phase.id}}>
        Remove
      </button>
    </li>
  </ul>

{{/each}}
